{
  "name": "time-warp-mobile",
  "version": "1.0.0",
  "description": "Time Warp IDE - Mobile App (React Native)",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "detox test e2e --configuration ios.sim.debug",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "clean": "rm -rf node_modules .expo .pytest_cache",
    "build:apk": "eas build --platform android --local",
    "build:ipa": "eas build --platform ios --local"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-native": "^0.72.0",
    "expo": "^49.0.0",
    "expo-splash-screen": "~0.26.0",
    "expo-status-bar": "~1.6.0",
    "react-navigation": "^6.1.0",
    "react-navigation-bottom-tabs": "^6.5.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "redux": "^4.2.1",
    "react-redux": "^8.1.3",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.0",
    "react-native-code-editor": "^0.3.0",
    "react-native-syntax-highlighter": "^1.0.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-paper": "^5.11.0",
    "@react-native-community/netinfo": "^11.0.0",
    "expo-file-system": "~15.4.0",
    "expo-sharing": "~12.0.0",
    "expo-av": "~13.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@testing-library/react-native": "^12.3.0",
    "@testing-library/jest-native": "^5.4.3",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-expo": "^49.0.0",
    "eslint": "^8.52.0",
    "eslint-config-react-native": "^6.0.0",
    "prettier": "^3.0.0",
    "detox": "^20.14.0",
    "detox-cli": "^20.14.0"
  },
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
